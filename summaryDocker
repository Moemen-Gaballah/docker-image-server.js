docker run postgres:9.6
docker pull redis
docker images
docker run redis
docker run -d redis // detached منفصل
docker stop ID // docker stop = stops the container
docker start // docker start = starts stopped container
docker ps // docker ps = lists running container
docker ps -a // docker ps -a = lists running and stopped container
docker run redis:4.0 // docker run = pulls image and starts container
docker run -p8088:8000 nameImage // change port image from 8000 to 8088 
docker run -p8088:8000 nameImage -d //

docker run -p6001:6379 -d redis:4.0 // run container redis on port 6001
docker logs containerId //
docker run -d -p6001:6379 --name redis-older redis:4.0 // run container on port 6001 and name it to(redis-order)

docker exec -it containerId /bin/bash


